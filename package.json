{
    "name": "@nimble-ts/cli",
    "version": "1.1.5",
    "description": "The CLI tool for Nimble framework",
    "author": "Eric Ferreira",
    "repository": "https://github.com/ericferreira1992/nimble-cli",
    "license": "MIT",
    "main": "./lib/cli.js",
    "bin": {
        "nb": "bin/nb"
    },
    "keywords": [
        "nimble",
        "nb",
        "cli",
        "nimble-ts"
    ],
    "scripts": {
        "start": "npm run build && npm run global",
        "build": "npm run clean:some && tsc -p . && node copy",
        "global": "npm i -g && nb",
        "clean:some": "rm -rf ./lib ./docs",
        "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
        "refresh": "npm run clean:all && npm install",
        "install-package": "npm run build && npm i -g",
        "publish-package": "npm run build && npm publish --access public",
        "docs": "typedoc --out docs ./src"
    },
    "dependencies": {
        "@types/fs-extra": "^8.0.0",
        "address": "^1.1.2",
        "chalk": "^3.0.0",
        "clean-webpack-plugin": "^2.0.1",
        "copy-webpack-plugin": "^5.0.2",
        "cpx": "^1.5.0",
        "cross-spawn": "^7.0.1",
        "css-loader": "^2.1.1",
        "detect-port-alt": "^1.1.6",
        "figlet": "^1.2.4",
        "file-loader": "^3.0.1",
        "filesize": "^6.0.1",
        "fs-extra": "^8.1.0",
        "gzip-size": "^5.1.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "inquirer": "^7.0.0",
        "inversify": "^5.0.1",
        "is-root": "^2.1.0",
        "kleur": "^3.0.3",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.11.0",
        "postcss-loader": "^3.0.0",
        "prerender-spa-plugin": "^3.4.0",
        "recursive-readdir": "^2.2.2",
        "reflect-metadata": "^0.1.13",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "ts-loader": "^6.2.1",
        "tslib": "^1.10.0",
        "webpack": "^4.28.1",
        "webpack-dev-server": "^3.1.14"
    },
    "devDependencies": {
        "@liftr/tscov": "^1.4.3",
        "@types/figlet": "^1.2.0",
        "@types/inquirer": "^6.5.0",
        "@types/node": "^12.12.7",
        "ts-node": "^8.5.0",
        "tslint": "^5.20.1",
        "typedoc": "^0.15.0",
        "typescript": "^3.7.2"
    }
}
