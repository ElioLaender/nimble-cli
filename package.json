{
	"name": "@nimble-ts/cli",
	"version": "1.3.4",
	"description": "The CLI tool for Nimble framework",
	"author": "Eric Ferreira",
	"repository": "https://github.com/ericferreira1992/nimble-cli",
	"license": "MIT",
	"main": "./lib/cli.js",
	"bin": {
		"nb": "bin/nb"
	},
	"keywords": [
		"nimble",
		"nb",
		"cli",
		"nimble-ts"
	],
	"scripts": {
		"start": "npm run build && npm run global",
		"build": "npm run clean:some && tsc -p . && node copy",
		"global": "npm i -g && nb",
		"clean:some": "rm -rf ./lib ./docs",
		"clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
		"refresh": "npm run clean:all && npm install",
		"install-package": "npm run build && npm i -g",
		"publish-package": "npm run build && npm publish --access public",
		"docs": "typedoc --out docs ./src"
	},
	"dependencies": {
		"@types/fs-extra": "^9.0.1",
		"address": "^1.1.2",
		"base-href-webpack-plugin": "^2.0.0",
		"chalk": "^4.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"compression-webpack-plugin": "^5.0.1",
		"copy-webpack-plugin": "^5.0.2",
		"cpx": "^1.5.0",
		"cross-spawn": "^7.0.3",
		"css-loader": "^4.0.0",
		"detect-port-alt": "^1.1.6",
		"figlet": "^1.5.0",
		"file-loader": "^6.0.0",
		"filesize": "^6.1.0",
		"fs-extra": "^9.0.1",
		"gzip-size": "^5.1.1",
		"html-loader": "^1.1.0",
		"html-webpack-plugin": "^3.0.0",
		"inquirer": "^7.3.3",
		"inversify": "^5.0.1",
		"is-root": "^2.1.0",
		"kleur": "^4.0.2",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"postcss-loader": "^3.0.0",
		"prerender-spa-plugin": "^3.4.0",
		"recursive-readdir": "^2.2.2",
		"reflect-metadata": "^0.1.13",
		"sass-loader": "^9.0.2",
		"semver": "^7.3.2",
		"style-loader": "^1.2.1",
		"ts-loader": "^8.0.1",
		"webpack": "^4.44.0",
		"webpack-dev-server": "^3.11.0"
	},
	"devDependencies": {
		"@liftr/tscov": "^1.4.7",
		"@types/figlet": "^1.2.0",
		"@types/inquirer": "^6.5.0",
		"@types/node": "^12.12.7",
		"ts-node": "^8.10.2",
		"tslib": "^2.0.0",
		"tslint": "^6.1.2",
		"typedoc": "^0.17.8",
		"typescript": "^3.9.7"
	}
}
